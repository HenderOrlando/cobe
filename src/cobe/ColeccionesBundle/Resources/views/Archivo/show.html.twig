{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Archivo</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Url</th>
                <td>{{ entity.url }}</td>
            </tr>
            <tr>
                <th>Fullurl</th>
                <td>{{ entity.fullUrl }}</td>
            </tr>
            <tr>
                <th>Size</th>
                <td>{{ entity.size }}</td>
            </tr>
            <tr>
                <th>Ext</th>
                <td>{{ entity.ext }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('colecciones_archivo') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('colecciones_archivo_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
