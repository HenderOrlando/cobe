{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Historial</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Navegador</th>
                <td>{{ entity.navegador }}</td>
            </tr>
            <tr>
                <th>Entityname</th>
                <td>{{ entity.entityName }}</td>
            </tr>
            <tr>
                <th>Entityid</th>
                <td>{{ entity.entityId }}</td>
            </tr>
            <tr>
                <th>Ipv4</th>
                <td>{{ entity.ipv4 }}</td>
            </tr>
            <tr>
                <th>Ipv6</th>
                <td>{{ entity.ipv6 }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('usuarios_historial') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('usuarios_historial_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
